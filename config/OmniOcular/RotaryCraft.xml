<oo>
    <tileentity id="RC.*">
        <line>
		if(nbt['power']!=0){if(nbt['power']!=undefined){
            return translate('hud.msg.RotaryCraft.Power')+": "+(nbt['power']/1000).toFixed(3)+"kW"}}
        </line>
        <line>
		if(nbt['torque']!=0){if(nbt['torque']!=undefined){
            return translate('hud.msg.RotaryCraft.Torque')+": "+nbt['torque']+" Nm"}}
        </line>
        <line>
		if(nbt['omega']!=0){if(nbt['omega']!=undefined){
            return translate('hud.msg.RotaryCraft.Speed')+": "+nbt['omega']+" rad/s"}}
        </line>
    </tileentity>
	<tileentity id="RC.*">
		<line>
		if(nbt['power']==0){
			return translate('hud.msg.RotaryCraft.Power')+": "+"0W"}
		</line>
		<line>
		if(nbt['torque']==0){
			return translate('hud.msg.RotaryCraft.Torque')+": "+"0 Nm"}
		</line>
		<line>
		if(nbt['omega']==0){
			return translate('hud.msg.RotaryCraft.Speed')+": "+"0 rad/s"}
		</line>
	</tileentity>
</oo>
