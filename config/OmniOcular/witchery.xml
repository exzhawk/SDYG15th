<oo>
    <tileentity id="witchery:altar">
        <line>
            return nbt['Power'].toFixed(0)+" / "+nbt['MaxPower']*nbt['PowerScale']+" (x"+nbt['PowerScale']+")"
        </line>
    </tileentity>
	<tileentity id="witchery:leechchest">
        <line>
            return translate('hud.msg.witchery.Owner')+" : "+nbt['WITCPlayers'][0]['Player']
        </line>
    </tileentity>
</oo>
